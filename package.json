    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.16",
    "framer-motion": "^10.18.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
